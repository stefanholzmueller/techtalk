package java8.functionalinterfaces;

public interface Traits {
	interface Trait {
		String getX();

		String getY();

		default void doXY() {
			System.out.println(getX() + getY());
		}
	}

	interface MyTrait extends Trait {
		default String getY() {
			return "Y";
		}
	}

	class MyClass implements MyTrait {

		@Override
		public String getX() {
			return "X";
		}

	}
	public static void main(String[] args) {
		new MyClass().doXY();
	}
}
